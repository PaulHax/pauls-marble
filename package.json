{
  "name": "Pauls-Marble",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm run clean && parcel src/index.html",
    "build": "npm run clean && parcel build src/index.html --public-url ./",
    "clean": "rimraf ./dist ./.cache",
    "predeploy": "rollup -c rollup.examples.config.js && shx mkdir -p .ghpages && shx cp -r examples/index.html examples/assets examples/build.js .ghpages",
    "trydeploy": "npm run predeploy && sirv start .ghpages",
    "deploy": "npm run predeploy && gh-pages -d .ghpages && shx rm -rf .ghpages"
  },
  "keywords": [
    "Parcel",
    "TypeScript",
    "ThreeJs",
    "Three.js"
  ],
  "license": "MIT",
  "devDependencies": {
    "glslify-bundle": "^5.1.1",
    "glslify-deps": "^1.3.1",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.2.1",
    "rimraf": "^3.0.0",
    "typescript": "^3.6.3",
    "shx": "^0.2.2",
    "gh-pages": "^2.1.1",
    "sirv-cli": "^0.4.4"
  },
  "dependencies": {
    "animejs": "^3.1.0",
    "three": "^0.112.0"
  },
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "assets",
        "staticOutDir": "assets"
      }
    ]
  }
}
